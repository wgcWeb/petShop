/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.yellow{
  color: #ff4d00;
}

/* 无数据 */
.no-result{
  background-color: #f5f5f5;
  height: 100vh;
  width: 100%;
}
.no-content{
  padding-top: 100rpx;
  margin-bottom: 100rpx;
  text-align: center;
}
.no-content image{
  width: 160rpx;
  height: 160rpx;
}
.no-content text{
  display: block;
  font-size: 40rpx;
  margin: 40rpx;
  color: #666;
}
.no-result .go-other{
  width: 500rpx;
  height: 100rpx;
  color: #f2873b;
  border: 2rpx solid #f2873b;
  margin-top: 20rpx auto;
  border-radius: 20rpx;
}

/* 阿里icon */
@font-face {font-family: "iconfont";
  src: url('//at.alicdn.com/t/font_2113171_79mek48y9d.eot?t=1601888395096'); /* IE9 */
  src: url('//at.alicdn.com/t/font_2113171_79mek48y9d.eot?t=1601888395096#iefix') format('embedded-opentype'), /* IE6-IE8 */
  url('data:application/x-font-woff2;charset=utf-8;base64,') format('woff2'),
  url('//at.alicdn.com/t/font_2113171_79mek48y9d.woff?t=1601888395096') format('woff'),
  url('//at.alicdn.com/t/font_2113171_79mek48y9d.ttf?t=1601888395096') format('truetype'), /* chrome, firefox, opera, Safari, Android, iOS 4.2+ */
  url('//at.alicdn.com/t/font_2113171_79mek48y9d.svg?t=1601888395096#iconfont') format('svg'); /* iOS 4.1- */
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.iconarrowdropdown-copy-copy:before {
  content: "\e602";
}

.icon-share:before {
  content: "\e6f5";
}

.icon-wechat:before {
  content: "\e621";
}

.icon-home:before {
  content: "\e600";
}

.icon-cart:before {
  content: "\e601";
}

.icon-map:before {
  content: "\e608";
}

.iconfont{
  font-size: 22px;
}