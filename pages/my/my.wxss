/* pages/my/my.wxss */
page{
  height: 100%;
}
.page-box{
  background-color: #f5f5f5;
  height: 100%;
  padding-top: 20rpx;
}
.btn-login{
  width: 100% !important;
  padding: 0;

}
.login-info{
  display: flex;
  background-color: #fff;
  padding: 20rpx 30rpx;
}
.login-info image{
  width: 160rpx;
  height: 160rpx;
  background-color: #f5f5f5;
}
.login-info .login{
  margin-left: 30rpx;
}
.login-info .login .top{
  font-size: 36rpx;
  margin-top: 30rpx;
}
.login-info .login .desc{
  font-size: 30rpx;
  color: #999;
  margin-top: 10rpx;
}
.member-box{
  background-color: #fff;
  margin-top: 30rpx;
}
.member-box .item{
  height: 88rpx;
  line-height: 88rpx;
  font-size: 32rpx;
  border-bottom: 2rpx solid #eee;
  padding: 0 30rpx;
}
.fn-right{
  float: right;
}